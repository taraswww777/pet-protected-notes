{
  "name": "protected-notes",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "fe",
    "be"
  ],
  "scripts": {
    "build": "npm run fe:build && npm run be:build",
    "checks": "npm run fe:lint && npm run be:lint",
    "be": "echo BE scripts === === === === === === === === ===",
    "be:build": "npm -w protected-notes-be run build",
    "be:start": "npm -w protected-notes-be run start",
    "be:start:tsx": "npm -w protected-notes-be run start:tsx",
    "be:dev": "npm -w protected-notes-be run dev",
    "be:lint": "npm -w protected-notes-be run lint",
    "be:lint:fix": "npm -w protected-notes-be run lint:fix",
    "be:db:migrate": "npm -w protected-notes-be run db:migrate",
    "be:db:generate": "npm -w protected-notes-be run db:generate",
    "db:seeding": "npm -w protected-notes-be run db:seeding",
    "fe": "echo FE scripts === === === === === === === === ===",
    "fe:dev": "npm -w protected-notes-fe run dev",
    "fe:build": "npm -w protected-notes-fe run build",
    "fe:lint": "npm -w protected-notes-fe run lint",
    "fe:lint:fix": "npm -w protected-notes-fe run lint:fix",
    "fe:preview": "npm -w protected-notes-fe run preview",
    "docker:dev": "docker-compose up -d",
    "docker:start": "docker-compose up",
    "docker:build": "docker build -t protected-notes .",
    "docker:stop": "docker-compose down"
  },
  "devDependencies": {
    "@faker-js/faker": "9.6.0",
    "@iconify-json/vscode-icons": "1.2.23",
    "@iconify/json": "2.2.341",
    "@iconify/tailwind": "1.2.0",
    "@reduxjs/toolkit": "2.2.2",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.4.8",
    "@types/bcrypt": "6.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/lodash": "4.17.14",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@vitejs/plugin-react-swc": "3.8.1",
    "autoprefixer": "10.4.19",
    "axios": "1.11.0",
    "bcrypt": "6.0.0",
    "dotenv": "17.2.0",
    "drizzle-kit": "0.31.4",
    "drizzle-orm": "0.44.3",
    "drizzle-seed": "0.3.1",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "fastify": "5.3.3",
    "fastify-cli": "7.4.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "pg": "8.16.3",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "5.0.0",
    "react-redux": "9.1.0",
    "react-router": "7.6.0",
    "tailwindcss": "3.4.1",
    "tailwindcss-plugin-icons": "2.2.2",
    "ts-node": "10.9.2",
    "tsx": "4.19.3",
    "typescript": "5.4.3",
    "typescript-eslint": "8.29.0",
    "vite": "6.3.5",
    "zod": "4.0.14"
  },
  "engines": {
    "node": "22"
  }
}
