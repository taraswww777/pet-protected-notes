{
  "name": "protected-notes-be",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server.js",
    "start:tsx": "tsx ./src/server.ts",
    "start:tsx:watch": "tsx --watch ./src/server.ts",
    "dev": "tsx --watch ./src/server.ts --noEmit",
    "lint": "eslint -c ./eslint.config.mjs",
    "lint:fix": "eslint --fix",
    "db:seeding": "tsx ./src/seeding.ts --seed",
    "db:migrate": "npx drizzle-kit migrate",
    "db:generate": "npx drizzle-kit generate --config=./drizzle.config.ts"
  },
  "devDependencies": {
    "@electric-sql/pglite": "0.3.5",
    "@types/bcrypt": "6.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/lodash": "4.17.14",
    "@types/pg": "8.15.5",
    "bcrypt": "6.0.0",
    "csv-stringify": "6.6.0",
    "dotenv": "17.2.0",
    "drizzle-kit": "0.31.4",
    "drizzle-orm": "0.44.3",
    "drizzle-seed": "0.3.1",
    "drizzle-zod": "0.8.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "fastify": "5.3.3",
    "fastify-cli": "7.4.0",
    "jsonwebtoken": "9.0.2",
    "pg": "8.16.3",
    "ts-node": "10.9.2",
    "tsx": "4.19.3",
    "uuid": "11.1.0",
    "zod": "4.0.14"
  }
}
